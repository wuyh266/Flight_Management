QWidget {
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
    color: #ffffff; /* 全局白色文字 */
}

QWidget#UserProfile {
    background-color: #2d2d2d; /* 全局深色背景 */
}

/* ================= 按钮统一风格 (复用 deal.ui 样式) ================= */
QPushButton {
    background-color: #606060;
    border: 1px solid #ffffff;
    border-radius: 6px;
    padding: 8px 16px;
    color: white;
    font-size: 14px;
    /* 模拟阴影效果 */
    box-shadow: inset 2px 2px 4px #484848,
                inset -2px -2px 4px #787878;
}

QPushButton:hover {
    background-color: #646464;
}

QPushButton:pressed {
    background-color: #404040;      
    border: 1px solid #555555;     
    padding-left: 10px;            
    padding-top: 10px;               
    padding-right: 6px;           
    padding-bottom: 6px;             

    box-shadow: inset 4px 4px 8px #202020,
                inset -4px -4px 8px #505050;
}

/* ================= 顶部导航栏 ================= */
QWidget#widget {
    background-color: #3b3b3b;
    border-bottom: 1px solid #555555;
}

/* 标题 */
QLabel#label {
    font-size: 18px;
    font-weight: bold;
    color: #ffffff;
}

/* ================= 卡片样式 ================= */
QFrame#frame, QFrame#frame_2 {
    background-color: #3b3b3b;
    border-radius: 12px;
    border: 1px solid #555555;
}

/* ================= 个人信息区 ================= */
/* 头像框 */
QLabel#label_2 {
    background-color: #505050;
    border-radius: 35px;
    border: 2px solid #606060;
    color: #cccccc;
    qproperty-alignment: AlignCenter;
}

/* 表单标签 */
QFormLayout QLabel {
    color: #cccccc;
    font-size: 14px;
}

/* 只读输入框 */
QLineEdit[readOnly=&quot;true&quot;] {
    background: transparent;
    border: none;
    border-bottom: 1px solid #555555;
    color: #ffffff;
    font-size: 14px;
    font-weight: 500;
    padding-bottom: 2px;
}</string>
