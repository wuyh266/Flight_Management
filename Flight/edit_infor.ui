   <string notr="true">/* ================= 全局窗口设置 ================= */
QDialog {
    background-color: #ffffff;
    color: #333333;
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
}

/* ================= 标签 (Label) 样式 ================= */
QLabel {
    color: #333333;
    font-size: 14px;
    font-weight: bold;
    background: transparent;
    border: none;
}

/* ================= 输入框 (LineEdit) 样式 ================= */
QLineEdit {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 4px;
    color: #000000;
    padding: 5px 8px;
    font-size: 14px;
}

QLineEdit:focus {
    border: 1px solid #0078d7;
}

/* 只读输入框样式 */
QLineEdit[readOnly=&quot;true&quot;] {
    background-color: #f7f7f7;
    color: #555555;
    border: 1px solid #dddddd;
}

/* ================= 按钮 (PushButton) 样式 ================= */
QPushButton {
    background-color: #ffffff;
    border: 1px solid #dcdcdc;
    border-bottom: 2px solid #b0b0b0;
    border-radius: 6px;
    color: #333333;
    font-size: 14px;
    padding: 6px 16px;
}

QPushButton:hover {
    background-color: #f0f8ff;
    border-color: #0078d7;
    color: #0078d7;
}

QPushButton:pressed {
    background-color: #e0e0e0;
    border-top: 2px solid #b0b0b0;
    border-bottom: none;
    padding-top: 8px;
    padding-bottom: 4px;
}

/* ================= 头像预览框样式 ================= */
