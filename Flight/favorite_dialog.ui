   <string notr="true">/* 全局背景和字体 */
QWidget#favorite_dialog {
    background-color: #f0f2f5;
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
}

/* 标题样式 */
QLabel#label_title {
    color: #2c3e50;
    font-size: 22px;
    font-weight: bold;
    padding-left: 10px;
}

/* 顶部按钮通用样式 */
QPushButton {
    background-color: #ffffff;
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    color: #606266;
    padding: 6px 16px;
    font-size: 14px;
}

QPushButton:hover {
    background-color: #ecf5ff;
    color: #409eff;
    border-color: #c6e2ff;
}

QPushButton:pressed {
    background-color: #d9ecff;
    border-color: #409eff;
}

/* 特殊按钮样式：搜索按钮（强调色） */
QPushButton#searchBtn {
    background-color: #409eff;
    color: white;
    border: none;
}
QPushButton#searchBtn:hover {
    background-color: #66b1ff;
}
QPushButton#searchBtn:pressed {
    background-color: #3a8ee6;
}

/* 分组框（搜索区域）样式 */
QGroupBox {
    background-color: #ffffff;
    border: 1px solid #e4e7ed;
    border-radius: 8px;
    margin-top: 10px;
    padding-top: 10px;
    font-size: 14px;
    color: #303133;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 10px;
    color: #409eff;
    font-weight: bold;
}

/* 输入框和下拉框样式 */
QLineEdit, QDateEdit, QComboBox {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    padding: 4px;
    color: #606266;
    background-color: #fff;
    min-height: 20px;
}
QLineEdit:focus, QDateEdit:focus, QComboBox:focus {
    border-color: #409eff;
}

/* 表格样式 */
QTableWidget {
    background-color: #ffffff;
    border: 1px solid #e4e7ed;
    border-radius: 8px;
    gridline-color: #ebeef5;
    selection-background-color: #ecf5ff;
    selection-color: #303133;
    font-size: 14px;
}
QHeaderView::section {
    background-color: #f5f7fa;
    color: #909399;
    padding: 8px;
    border: none;
    border-bottom: 1px solid #e4e7ed;
    border-right: 1px solid #e4e7ed;
    font-weight: bold;
}</string>
